{
  "name": "gpcbtinewweb-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/client",
    "apps/server"
  ],
  "scripts": {
    "dev": "concurrently -n server,client -c green,cyan \"npm:dev --workspace apps/server\" \"npm:dev --workspace apps/client\"",
    "build": "npm run build -w apps/client",
    "start": "npm run start -w apps/server"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
